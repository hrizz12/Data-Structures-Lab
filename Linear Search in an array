#include <stdio.h>
int main()
{
	int n,i,key,flag;
	flag = 0;
	int arr[n];
	scanf("%d",&n);
	for(i = 0; i<n;i++)
		{
		scanf("%d",&arr[i]);
		}
	scanf("%d",&key);
	for(i=0;i<n;i++)
		{
		if (arr[i]==key){
			
			flag=1;
			break;
		}
		}
	if(flag==1)
		printf("found at position %d\n",i);
	else
		printf("%d not found\n",key);
	return 0;
}
